{
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "test": {
      "presets": [
        ["env", {
          "modules": "commonjs"
        }],
        "next/babel"
      ]
    }
  },
  "plugins": [
    "transform-flow-strip-types",
    "babel-plugin-inline-import-graphql-ast",
    "ramda",
    ["module-resolver", {
      "root": ["./private", "public/semantic", "public/img"]
    }]
  ]
}
