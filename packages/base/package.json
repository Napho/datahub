{
  "name": "@devinit/dh-base",
  "version": "1.0.4",
  "description": "for datahub related libs & tools",
  "config": {
    "api": "http://212.111.41.68:9090/graphql",
    "old_datahub": "http://212.111.41.68:8888"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "pdfs"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/di-pdfs.git"
  },
  "peerDependencies": {
    "glamor": "^2.20.40"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "ts-jest": "22.4.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
