{
  "name": "@devinit/dh-app",
  "version": "3.0.2",
  "config": {
    "api": "http://212.111.41.68:9090/graphql",
    "old_datahub": "http://212.111.41.68:8888"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/datahub"
  },
  "publishConfig": {
    "access": "private"
  },
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "dev": "next src",
    "build:next": "next build src",
    "start": "next start src",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "gql-schema": "apollo-codegen introspect-schema $npm_package_config_api  --output schema.json",
    "gql-ts": "apollo-codegen generate ./src/**/query.graphql.ts --schema schema.json --target typescript --output ./src/types.ts",
    "types": "npm run gql-schema && npm run gql-ts && npm run cp-types-ui",
    "cp-types-ui": "cp ./src/types.ts ../ui/src/gql-types.ts",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n"
  },
  "dependencies": {
    "@devinit/datahub-api": "^1.0.2",
    "@devinit/dh-base": "^1.0.3",
    "@devinit/dh-ui": "^1.0.2",
    "@zeit/next-css": "^0.1.2",
    "apollo-client-preset": "^1.0.8",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.7.3",
    "intersection-observer": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.6",
    "lru-cache": "^4.1.1",
    "next": "^5.0.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.0",
    "semantic-ui-react": "^0.77.2"
  },
  "devDependencies": {
    "@zeit/next-typescript": "0.0.8",
    "apollo-codegen": "0.18.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "setupTestFrameworkScriptFile": "<rootDir>/private/lib/jest.config.js",
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/private/__mocks__/styleMock.js"
    }
  },
  "engines": {
    "node": ">=8.5"
  }
}
