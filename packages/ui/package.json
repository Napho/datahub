{
    "name": "@devinit/dh-ui",
    "version": "0.0.1",
    "description": "for creating datahub ui components",
    "main": "lib/index.js",
    "scripts": {
      "build": "NODE_ENV=production npm run build-css && cp -R static lib/ && tsc",
      "tsc": "tsc",
      "copy-chart-css": "cp node_modules/@devinit/charts/dist/di-charts.min.css static/",
      "lint": "tslint -c tslint.json 'pages/**/*.tsx' 'src/**/*.tsx' 'src/**/*.ts'",
      "storybook": "start-storybook -s static -p 6006",
      "patch": "release-it -i patch -n",
      "feature": "release-it -i minor -n",
      "breaking": "release-it -i major -n",
      "build-storybook": "build-storybook"
    },
    "keywords": [
      "ui", "datahub", "storybook"
    ],
    "author": "epicallan.al@gmail.com",
    "license": "ISC",
    "publishConfig": {
      "access": "public"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/devinit/di-pdfs.git"
    },
    "dependencies": {
      "@devinit/charts": "^1.6.47",
      "glamor": "^2.20.40",
      "glamorous": "^4.11.0",
      "ramda": "^0.25.0",
      "react": "^16.2.0",
      "react-dom": "^16.2.0"
    },
    "devDependencies": {
      "@storybook/addon-actions": "3.3.10",
      "@storybook/addon-links": "3.3.10",
      "@storybook/react": "3.3.10",
      "@types/node": "9.3.0",
      "@types/react": "16.0.28",
      "@types/storybook__react": "3.0.6",
      "concurrently": "3.5.1",
      "release-it": "6.1.1",
      "ts-loader": "3.3.1",
      "tslint": "5.9.1",
      "tslint-react": "3.4.0",
      "typescript": "2.6.2"
    }
  }
  