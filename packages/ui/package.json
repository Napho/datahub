{
  "name": "@devinit/dh-ui",
  "version": "0.0.1",
  "description": "for creating datahub ui components",
  "main": "lib/index.js",
  "scripts": {
    "release-it": "release-it",
    "build": "NODE_ENV=production npm run build-css && cp -R static lib/ && tsc",
    "tsc": "tsc",
    "copy-chart-css": "cp node_modules/@devinit/charts/dist/di-charts.min.css static/",
    "lint": "tslint -c tslint.json 'pages/**/*.tsx' 'src/**/*.tsx' 'src/**/*.ts'",
    "storybook": "start-storybook -s static -p 6006",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "ui",
    "datahub",
    "storybook"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/datagub.git"
  },
  "dependencies": {
    "@devinit/charts": "^1.6.47",
    "@devinit/dh-base": "1.0.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "mapbox-gl": "^0.43.0",
    "next": "^4.2.3",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-ui-react": "^0.77.2"
  }
}
