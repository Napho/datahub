{
  "name": "datahub",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf ./node_modules/.cache",
    "dev": "PORT=8888 BABEL_ENV=development npm run clean && node server.js",
    "build": "BABEL_ENV=production NODE_ENV=production npm run clean && next build",
    "start": "BABEL_ENV=production NODE_ENV=production node server",
    "lint": "eslint private server.js --ext .graphql --ext .js && npm run flow",
    "format": "prettier-eslint --write \"components/**/*.js\" \"lib/*.js\" \"pages/*.js\"",
    "flow": "flow",
    "test": "BABEL_ENV=test jest",
    "coverage": "BABEL_ENV=test jest && codecov",
    "storybook": "BABEL_ENV=development start-storybook -c private/.storybook -s public -p 6006 ",
    "build-storybook": "BABEL_ENV=production build-storybook -c private/.storybook -s public -o .out",
    "lessToJs": "node private/tools/lessToJs.js",
    "build-theme": "gulp  --gulpfile ./private/semantic/gulpfile.js && npm run lessToJs",
    "build-theme-w": "gulp watch --gulpfile ./private/semantic/gulpfile.js",
    "qql-schema": "apollo-codegen introspect-schema http://localhost:3000/graphql --output schema.json",
    "gql-flow": "apollo-codegen generate private/**/*.graphql --schema schema.json --target flow --output private/types/schema.flow.js",
    "gql": "npm run qql-schema &&  npm run gql-flow"
  },
  "peerDependencies": {
    "graphql": "^0.10.3"
  },
  "dependencies": {
    "@devinit/charts": "^1.2.9",
    "approximate-number": "^2.0.0",
    "babel-plugin-inline-import-graphql-ast": "^1.2.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "compression": "^1.6.2",
    "d3": "^4.9.1",
    "dragdealer": "^0.9.9",
    "express": "^4.15.3",
    "glamor": "^2.20.25",
    "glamorous": "^3.23.4",
    "graphql": "^0.10.3",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.0",
    "mapbox-gl": "^0.38.0",
    "next": "latest",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "redux": "^3.7.0",
    "redux-persist": "^4.8.1",
    "semantic-ui-react": "^0.69.0",
    "sw-precache-webpack-plugin": "^0.11.4"
  },
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.0.0",
    "@storybook/react": "3.0.0",
    "babel-eslint": "^7.2.3",
    "css-to-js-object": "^0.0.3",
    "enzyme": "^2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-flowtype-errors": "^3.3.0",
    "eslint-plugin-graphql": "^1.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.48.0",
    "fs-extra": "^3.0.1",
    "git-pre-hooks": "^1.2.0",
    "gulp": "^3.9.1",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "prettier": "^1.4.4",
    "prettier-eslint-cli": "^4.0.3",
    "react-test-renderer": "^15.5.4",
    "semantic-ui": "^2.2.10"
  },
  "git-pre-hooks": {
    "pre-push": "npm run test"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/private/__mocks__/styleMock.js"
    }
  }
}
